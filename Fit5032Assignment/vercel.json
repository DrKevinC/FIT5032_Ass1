{
    "version": 2,
    "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }],
    "builds": [
    {
      "src": "node_server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build",
        "devCommand": "npm run dev"
      }
    }
  ]
}